use ratarsedberrypi;

CREATE TABLE measurements (
id int,
keg CHAR(1) NOT NULL,
time DATETIME NOT NULL,
temp FLOAT NOT NULL,
weight FLOAT NOT NULL
);

INSERT INTO `ratarsedberrypi`.`measurements`
(`id`,
`keg`,
`time`,
`temp`,
`weight`)
VALUES
(1,
"L",
now(),
4.1,
17.45);



------

create table keg(
id int,
keg CHAR(1) NOT NULL,
time DATETIME NOT NULL,
zeroWeight FLOAT,
contents VARCHAR(20),
strength FLOAT,
active bool
)


INSERT INTO `ratarsedberrypi`.`keg`
(`id`,
`keg`,
`time`,
`zeroWeight`,
`contents`,
`strength`,
`active`)
VALUES
(1,
"L",
now(),
4.4,
"POWER Cider",
10.3,
1);
